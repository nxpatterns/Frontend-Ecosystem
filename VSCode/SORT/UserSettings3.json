{
  "search.searchEditor.doubleClickBehaviour": "goToLocation",
  "workbench.fontAliasing": "default",
  "mergeEditor.diffAlgorithm": "experimental",
  "terminal.integrated.smoothScrolling": true,

  "jest.autoRun": "false",
  "auto-close-tag.disableOnLanguage": ["typescript"],
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "entity.name.type.class.ts",
          "variable.other.constant.property.ts",
          "meta.brace.square.ts",
          "support.type.object.html-attribute"
        ],
        "settings": {
          "foreground": "#ffd000",
          "fontStyle": "bold"
        }
      },

      {
        "scope": [
          "punctuation.definition.typeparameters.begin.ts",
          "punctuation.definition.typeparameters.end.ts"
        ],
        "settings": {
          "foreground": "#ffffff",
          "fontStyle": "bold"
        }
      },

      {
        "scope": ["meta.objectliteral.ts"],
        "settings": {
          "foreground": "#ffffff",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["text.html.derivative", "constant.numeric.angular-directive"],
        "settings": {
          "foreground": "#aeff00",
          "fontStyle": "bold"
        }
      },

      {
        "scope": [
          "meta.brace.round.ts",
          "punctuation.definition.parameters.begin.ts",
          "punctuation.definition.parameters.end.ts"
        ],
        "settings": {
          "foreground": "#d8fc58",
          "fontStyle": "bold"
        }
      },

      {
        "scope": ["variable.language.this.ts"],
        "settings": {
          "foreground": "#bbff00",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["support.type.primitive.ts"],
        "settings": {
          "foreground": "#afc476",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "constant.language.undefined.ts",
          "constant.language.null.ts"
        ],
        "settings": {
          "foreground": "#a9f4f7",
          "fontStyle": "italic bold"
        }
      },
      {
        "scope": ["entity.name.type.ts"],
        "settings": {
          "foreground": "#7ffcbd",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "punctuation.accessor.optional.ts",
          "punctuation.accessor.ts",
          "keyword.operator.logical.ts",
          "keyword.operator.arithmetic.ts",
          "keyword.operator.spread.ts"
        ],
        "settings": {
          "foreground": "#ff00f2",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.other.unit.percentage.css",
          "keyword.other.unit.px.css"
        ],
        "settings": {
          "foreground": "#ff00f2",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.operator.expression.import.ts",
          "keyword.other.unit.px.css"
        ],
        "settings": {
          "foreground": "#8bff2c",
          "fontStyle": "bold underline"
        }
      },
      {
        "scope": ["keyword.operator.assignment.ts"],
        "settings": {
          "foreground": "#ffffff",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["support.function.ts", "support.class.builtin.ts"],
        "settings": {
          "foreground": "#2EDCFF",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["variable.other.property.ts"],
        "settings": {
          "foreground": "#f37bf9",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["storage.type.class.ts", "variable.language.this.ts"],
        "settings": {
          "foreground": "#2EDCFF",
          "fontStyle": "bold italic"
        }
      },

      {
        "scope": ["constant.language.boolean.true.ts"],
        "settings": {
          "foreground": "#29da06",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["constant.language.boolean.false.ts"],
        "settings": {
          "foreground": "#f83a79",
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "keyword.control.flow.ts",
          "variable.parameter.ts",
          "keyword.operator.type.annotation.ts",
          "variable.other.object.ts"
        ],
        "settings": {
          "foreground": "#8d6cfa",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["entity.other.ng-binding-name.ngIf.html", "string.regexp"],
        "settings": {
          "foreground": "#FFD000",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "punctuation.definition.ng-binding-name.begin.html",
          "punctuation.definition.ng-binding-name.end.html"
        ],
        "settings": {
          "foreground": "#EE00DA",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["entity.other.ng-binding-name.event.html"],
        "settings": {
          "foreground": "#ffd900",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["support.type.property-name.css"],
        "settings": {
          "foreground": "#ffd900",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["entity.name.tag.html"],
        "settings": {
          "foreground": "#EE00DA",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "entity.other.attribute-name.class.css",
          "entity.other.ng-binding-name.innerHTML.html",
          "entity.other.ng-binding-name.property.html"
        ],
        "settings": {
          "foreground": "#ffd900",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["punctuation.definition.tag.begin.html"],
        "settings": {
          "foreground": "#af7dff",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["meta.object-literal.key.ts"],
        "settings": {
          "foreground": "#a79db6",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["entity.name.function.pipe.ng"],
        "settings": {
          "foreground": "#f753f7",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["variable.other.readwrite.ts"],
        "settings": {
          "foreground": "#f785f7",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["punctuation.definition.tag.end.html"],
        "settings": {
          "foreground": "#85dcf7",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["string.quoted.double.html"],
        "settings": {
          "foreground": "#b292fc",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "string.quoted.single.ts",
          "string.quoted.double.json.comments"
        ],
        "settings": {
          "foreground": "#b9cfc8",
          "fontStyle": ""
        }
      },
      {
        "scope": ["support.type.property-name.json.comments"],
        "settings": {
          "foreground": "#fdece4",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["storage.type.ts"],
        "settings": {
          "foreground": "#8fe3ff",
          "fontStyle": "bold italic"
        }
      },
      {
        "scope": ["variable.other.readwrite.alias.ts"],
        "settings": {
          "foreground": "#ffbbea",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.control.import.ts",
          "keyword.control.export.ts",
          "keyword.control.from.ts",
          "storage.modifier.ts"
        ],
        "settings": {
          "foreground": "#fa03e5",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["storage.modifier.ts"],
        "settings": {
          "foreground": "#718ff3",
          "fontStyle": "italic bold"
        }
      },
      {
        "scope": ["string.template.ts"],
        "settings": {
          "foreground": "#fffd7c"
          // "fontStyle": "bold"
        }
      },
      {
        "scope": ["keyword.control.switch.ts"],
        "settings": {
          //"foreground": "#fffd7c"
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["meta.import.ts"],
        "settings": {
          "foreground": "#6f78f1"
        }
      },
      {
        "scope": ["keyword.operator.definiteassignment.ts"],
        "settings": {
          "foreground": "#4effd3",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["keyword.control.conditional.ts"],
        "settings": {
          "foreground": "#D8FC58",
          "fontStyle": "italic bold"
        }
      },
      {
        "scope": ["punctuation.definition.block.ts"],
        "settings": {
          "foreground": "#fd28cf",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["entity.other.attribute-name.html"],
        "settings": {
          "foreground": "#ffd000",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["entity.name.function.member"],
        "settings": {
          "foreground": "#ffd000",
          "fontStyle": "bold"
        }
      }
    ]
  },
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "enum": "#fffb00"
    }
  },
  "terminal.integrated.fontSize": 13,
  "gitlens.codeLens.enabled": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.enabled": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "printcode.fontSize": 11,
  "printcode.printFilePath": "relative",
  "security.workspace.trust.untrustedFiles": "open",
  "scss.lint.emptyRules": "ignore",
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "diffEditor.codeLens": false,
  "typescript.referencesCodeLens.enabled": false,
  "typescript.referencesCodeLens.showOnAllFunctions": false,
  "javascript.referencesCodeLens.showOnAllFunctions": false,
  "typescript.implementationsCodeLens.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "markdown-preview-enhanced.enableTypographer": true,
  "markdown-preview-enhanced.revealjsTheme": "moon.css",
  "editor.tabSize": 2,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "redhat.telemetry.enabled": false,
  "git.autofetch": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.wordWrap": "off",
  "editor.fontSize": 13.3,
  "editor.codeLensFontFamily": "Jetbrains Mono",
  "editor.lineHeight": 1.6,
  "js/ts.implicitProjectConfig.checkJs": true,

  "workbench.colorCustomizations": {
    "statusBar.background": "#00b3e6",
    "statusBar.foreground": "#15202b",
    "editor.background": "#1B262C",
    "editorUnnecessaryCode.border": "#fd7403dc",
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f",
    "activityBar.activeBackground": "#1accff",
    "activityBar.activeBorder": "#df00ad",
    "activityBar.background": "#1accff",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#df00ad",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#1accff",
    "statusBarItem.hoverBackground": "#008bb3",
    "statusBarItem.remoteBackground": "#00b3e6",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#00b3e699",
    "titleBar.inactiveForeground": "#15202b99",
    "commandCenter.border": "#15202b99",
    "tab.activeBorder": "#9102ff",
    "tab.activeBorderTop": "#9102ff",
    "tab.activeBackground": "#6403ad",
    "tab.activeForeground": "#ffffff",
    "tab.border": "#6403ad",
    "tab.inactiveForeground": "#ffbf00",
    "editor.selectionBackground": "#d902c377"
  },
  "terminal.integrated.gpuAcceleration": "off",
  "workbench.colorTheme": "Gitkraken Dark",
  "workbench.iconTheme": "material-icon-theme",
  "gitlens.views.branches.branches.layout": "list",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.typographer": true,
  "prettier.proseWrap": "always",
  "diffEditor.wordWrap": "off",
  "editor.guides.indentation": false,
  "editor.guides.bracketPairs": false,
  "http.systemCertificates": false,
  "[groovy]": {
    "editor.defaultFormatter": "NicolasVuillamy.vscode-groovy-lint"
  },
  "turboConsoleLog.delimiterInsideMessage": "->",
  "turboConsoleLog.logMessagePrefix": "",
  "turboConsoleLog.quote": "`",
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.insertEnclosingFunction": false,
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "editor.inlayHints.enabled": "on",
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
  "markdown-preview-enhanced.previewTheme": "one-dark.css",
  "editor.stickyScroll.enabled": false,
  "explorer.confirmDelete": false,
  "workbench.startupEditor": "none",
  "workbench.productIconTheme": "fluent-icons",
  "editor.inlineSuggest.enabled": true,
  "window.commandCenter": false
}
