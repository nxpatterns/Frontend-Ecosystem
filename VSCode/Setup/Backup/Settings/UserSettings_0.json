{
  "auto-close-tag.disableOnLanguage": ["typescript"],
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "entity.name.type.class.ts",
          "variable.other.constant.property.ts",
          "meta.brace.square.ts",
          "entity.name.function.ts"
        ],
        "settings": {
          "foreground": "#ffd000",
          "fontStyle": "bold"
        }
      },

      {
        "scope": [
          "punctuation.definition.typeparameters.begin.ts",
          "punctuation.definition.typeparameters.end.ts"
        ],
        "settings": {
          "foreground": "#ffffff",
          "fontStyle": "bold"
        }
      },

      {
        "scope": [
          "meta.brace.round.ts",
          "punctuation.definition.parameters.begin.ts",
          "punctuation.definition.parameters.end.ts"
        ],
        "settings": {
          "foreground": "#d8fc58",
          "fontStyle": "bold"
        }
      },

      {
        "scope": ["variable.language.this.ts"],
        "settings": {
          "foreground": "#bbff00",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["support.type.primitive.ts"],
        "settings": {
          "foreground": "#afc476",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "constant.language.undefined.ts",
          "constant.language.null.ts"
        ],
        "settings": {
          "foreground": "#a9f4f7",
          "fontStyle": "italic bold"
        }
      },
      {
        "scope": ["entity.name.type.ts"],
        "settings": {
          "foreground": "#3cf2ff",
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "punctuation.accessor.optional.ts",
          "punctuation.accessor.ts",
          "keyword.operator.logical.ts",
          "keyword.operator.arithmetic.ts",
          "keyword.operator.spread.ts"
        ],
        "settings": {
          "foreground": "#ff00f2",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.other.unit.percentage.css",
          "keyword.other.unit.px.css"
        ],
        "settings": {
          "foreground": "#ff00f2",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["keyword.operator.assignment.ts"],
        "settings": {
          "foreground": "#ffffff",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["support.function.ts", "support.class.builtin.ts"],
        "settings": {
          "foreground": "#2EDCFF",
          "fontStyle": "bold"
        }
      },

      {
        "scope": ["constant.language.boolean.true.ts"],
        "settings": {
          "foreground": "#29da06",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["constant.language.boolean.false.ts"],
        "settings": {
          "foreground": "#f76495",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.control.flow.ts",
          "variable.parameter.ts",
          "keyword.operator.type.annotation.ts",
          "variable.other.object.ts"
        ],
        "settings": {
          "foreground": "#beb0fa",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["entity.other.ng-binding-name.ngIf.html"],
        "settings": {
          "foreground": "#FFD000",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "punctuation.definition.ng-binding-name.begin.html",
          "punctuation.definition.ng-binding-name.end.html"
        ],
        "settings": {
          "foreground": "#EE00DA",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["entity.other.ng-binding-name.event.html"],
        "settings": {
          "foreground": "#ffd900",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["support.type.property-name.css"],
        "settings": {
          "foreground": "#ffd900",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["entity.name.tag.html"],
        "settings": {
          "foreground": "#EE00DA",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "entity.other.attribute-name.class.css",
          "entity.other.ng-binding-name.innerHTML.html",
          "entity.other.ng-binding-name.property.html"
        ],
        "settings": {
          "foreground": "#ffd900",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "punctuation.definition.tag.begin.html",
          "meta.object-literal.key.ts"
        ],
        "settings": {
          "foreground": "#af7dff",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["entity.name.function.pipe.ng"],
        "settings": {
          "foreground": "#f753f7",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["punctuation.definition.tag.end.html"],
        "settings": {
          "foreground": "#f785f7",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["string.quoted.double.html", "variable.other.readwrite.ts"],
        "settings": {
          "foreground": "#b292fc",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "string.quoted.single.ts",
          "string.quoted.double.json.comments"
        ],
        "settings": {
          "foreground": "#b9cfc8",
          "fontStyle": ""
        }
      },
      {
        "scope": ["support.type.property-name.json.comments"],
        "settings": {
          "foreground": "#fdece4",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["storage.type.ts"],
        "settings": {
          "foreground": "#77aeff",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.control.import.ts",
          "keyword.control.export.ts",
          "keyword.control.from.ts",
          "storage.modifier.ts"
        ],
        "settings": {
          "foreground": "#fa03e5",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["string.template.ts"],
        "settings": {
          "foreground": "#fffd7c"
          // "fontStyle": "bold"
        }
      },
      {
        "scope": ["keyword.control.switch.ts"],
        "settings": {
          //"foreground": "#fffd7c"
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["meta.import.ts"],
        "settings": {
          "foreground": "#6f78f1"
        }
      },
      {
        "scope": ["keyword.control.conditional.ts"],
        "settings": {
          "foreground": "#D8FC58",
          "fontStyle": "italic bold"
        }
      },
      {
        "scope": ["punctuation.definition.block.ts"],
        "settings": {
          "foreground": "#fd28cf",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "entity.name.function.ts",
          "entity.other.attribute-name.html"
        ],
        "settings": {
          "foreground": "#ffd000",
          "fontStyle": "bold"
        }
      }
    ]
  },
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "enum": "#fffb00"
    }
  },
  "terminal.integrated.fontSize": 13,
  "gitlens.codeLens.enabled": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.enabled": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "printcode.fontSize": 11,
  "printcode.printFilePath": "relative",
  "security.workspace.trust.untrustedFiles": "open",
  "scss.lint.emptyRules": "ignore",
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "diffEditor.codeLens": false,
  "typescript.referencesCodeLens.enabled": false,
  "typescript.referencesCodeLens.showOnAllFunctions": false,
  "javascript.referencesCodeLens.showOnAllFunctions": false,
  "typescript.implementationsCodeLens.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "markdown-preview-enhanced.codeBlockTheme": "github.css",
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "markdown-preview-enhanced.enableTypographer": true,
  "markdown-preview-enhanced.revealjsTheme": "moon.css",
  "editor.tabSize": 2,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "redhat.telemetry.enabled": false,
  "git.autofetch": true,
  "grammarly.audience": "expert",
  "grammarly.autoActivate": false,
  "grammarly.dialect": "british",
  "grammarly.domain": "creative",
  "grammarly.showExamples": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.wordWrap": "on",
  "editor.fontSize": 13,
  "editor.codeLensFontFamily": "Jetbrains Mono",
  "editor.lineHeight": 1.6,
  "js/ts.implicitProjectConfig.checkJs": true,

  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#3a8cd8",
    "statusBar.background": "#284764",
    "statusBar.foreground": "#c8ff00",
    "editor.background": "#1B262C",
    // "activityBarBadge.background": "#fac928",
    "editorUnnecessaryCode.border": "#fd7403dc"
    // "editor.selectionBackground": "#827983dc",
    // "editor.wordHighlightBorder": "#fac928",
    // "sideBar.border": "#3c456e",
    // "sideBar.foreground": "#ffffffaa",
    // "sideBarSectionHeader.border": "#3c456e",
    //"terminal.background": "#271600"
    //"terminal.tab.activeBorder": "#b913ccdc"
    // "tab.activeBackground": "#003f64",
    // "tab.activeForeground": "#ffffff",
    // "tab.activeBorder": "#fac928"
  },
  "terminal.integrated.gpuAcceleration": "off",
  "workbench.colorTheme": "Material Monokai High Contrast",
  "workbench.iconTheme": "material-icon-theme",
  "gitlens.views.branches.branches.layout": "list"
}
